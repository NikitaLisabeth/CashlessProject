<Window x:Class="nmct.ba.cashlessproject.KassaUI.NummerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="NummerWindow" Height="350" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="29*"/>
            <RowDefinition Height="292*"/>
        </Grid.RowDefinitions>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Center" Height="120" Grid.Row="1" Stroke="DarkGray" VerticalAlignment="center" Width="242"/>
        <TextBox Height="23" Grid.Row="1" TextWrapping="Wrap" HorizontalAlignment="Center" Text="TextBox" VerticalAlignment="Center" Width="120" Margin="166,134.2,207.6,134.8"/>
        <Button Content="Go" Grid.Row="1" Margin="286,134.2,170.6,134.8"/>
        <TextBlock Style="{StaticResource TextblockStyle}" Text="Geef hier uw nummer in:" Grid.Row="1" Margin="143,104.2,145.6,162.8"/>
    </Grid>
</Window>
